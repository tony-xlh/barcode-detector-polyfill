{
  "name": "@aaronclong/barcode-detection-polyfill",
  "type": "module",
  "version": "0.1.0",
  "description": "Polyfill for the Barcode Detection API based on Dynamsoft Barcode Reader or ZXing.",
  "homepage": "https://github.com/aaronclong/barcode-detector-polyfill/#readme",
  "license": "MIT",
  "author": {
    "name": "Aaron Long"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": {
    "url": "https://github.com/aaronclong/barcode-detector-polyfill/issues"
  },
  "files": [
    "dist/*.js",
    "dist/*.js.map",
    "dist/*.cjs",
    "dist/*.cjs.map",
    "dist/*.mjs",
    "dist/*.mjs.map",
    "dist/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronclong/barcode-detector-polyfill"
  },
  "contributors": [
    {
      "name": "Xu Lihang",
      "url": "https://github.com/xulihang/barcode-detector-polyfill"
    }
  ],
  "exports": {
    "types": "./dist/*.d.ts",
    "import": "./dist/*.mjs",
    "require": "./dist/*.cjs",
    "default": "./dist/*.mjs"
  },
  "scripts": {
    "dev": "microbundle watch",
    "install:peers": "npx npm-install-peers -- -y",
    "build": "rollup -c rollup.config.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "rollup": "^3.27.0",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "barcode",
    "barcode-detector",
    "ean",
    "datamatrix",
    "pdf417",
    "qrcode"
  ],
  "peerDependencies": {
    "@zxing/browser": "^0.1.4",
    "@zxing/library": "^0.20.0"
  }
}
